/*
   Expected output:
   0
   30
   30
   0

   Expected stack:
   10  20  0
*/

begin 
   x : int,
   y : int,
   p : bool,
   function f(x : int, y : int, p : bool) : int = {
      print p;
      return x + y
   },
   function g(p : bool) : bool = {
      print x + y;
      return p
   };
   x := 10;
   y := 20;
   p := false;
   print f(x, y, p);
   print g(p)
end